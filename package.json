{
  "name": "input-area",
  "version": "0.0.1",
  "description": "Jupyter Input Area",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "mkdirp lib && mkdirp dist && npm run clean",
    "test": "mocha --recursive --compilers js:babel/register",
    "build": "npm run build:es5 && npm run build:browser",
    "build:es5": "babel src --out-dir lib/ --source-maps",
    "build:browser": "browserify lib/index.js --outfile dist/input-area.js",
    "prepublish": "npm run build",
    "watch": "parallelshell 'npm run watch:test -s' 'npm run watch:build -s'",
    "watch:build": "nodemon -q -w src/ --ext '.' --exec 'npm run build'",
    "watch:test": "npm run test -- -w",
    "clean": "rimraf lib/* dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupyter/jupyter-js-input-area.git"
  },
  "keywords": [
    "jupyter",
    "input",
    "input-area"
  ],
  "author": "Jupyter Project",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-input-area/issues"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-input-area#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "jsdom": "^6.5.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "navigator": "^1.0.1",
    "nodemon": "^1.4.0",
    "parallelshell": "^2.0.0",
    "rimraf": "^2.4.2"
  },
  "dependencies": {
    "codemirror": "^5.7.0",
    "fast-diff": "^1.0.1",
    "node-uuid": "^1.4.3",
    "phosphor-signaling": "^1.1.1",
    "underscore": "^1.8.3"
  }
}
